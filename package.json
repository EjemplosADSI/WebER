{
  "name": "weber",
  "version": "1.0.0",
  "description": "Ejemplo MVC POO",
  "main": "index.js",
  "scripts": {
    "buildcss": "node-sass --output-style compressed -r resources/scss/ -o views/public/css/",
    "buildjs": "uglifyjs-folder resources/js/ -eo views/public/js/",
    "buildimg": "copyfiles -u 2 resources/img/*.* views/public/img/",
    "build": "npm run buildcss & npm run buildjs & npm run buildimg"
  },
