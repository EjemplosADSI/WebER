  "bugs": {
    "url": "https://github.com/EjemplosADSI/WebER/issues"
  },
  "homepage": "https://github.com/EjemplosADSI/WebER#readme",
  "devDependencies": {},
  "dependencies": {
    "copyfiles": "^2.3.0",
    "node-sass": "^4.14.1",
    "postcss": "*",
    "sass-lint": "*",
    "uglify-js": "^3.10.0",
    "uglifyjs-folder": "^2.0.0"
  }
}
